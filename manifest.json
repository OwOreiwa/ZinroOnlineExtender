{
    "manifest_version": 2,

    "name": "人狼Onlineえくすてんだー",
    "version": "0.0.1",

    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },

    "content_scripts": [{
        "matches": ["https://zinro.net/*"],
        "js": ["jquery-3.6.0.min.js","script.js"]
    }],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
		"default_title": ""
	},
	"permissions": [
		"tabs",
		"background",
		"https://zinro.net/*"
	]
}